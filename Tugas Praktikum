package array;
public class Praktikum1 {
    public static void main(String[] args) {
        /* int [5] bilangan = {1,2,3,4,5}; akan error karena didalam kurung siku harusnya dikosongi
        System.out.println(bilangan[6]); akan error karena hanya ada 4 indeks */
        int bilangan [] = {1,2,3,4,5};
        for (int i = 0; i < bilangan.length; i++) {
            System.out.print(bilangan[i]);
            System.out.print(" ");
            
        }
    }
